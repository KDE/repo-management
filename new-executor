refname="$1"
oldrev="$2"
newrev="$3"

revdiff="$newrev"
if [ $oldrev != "0000000000000000000000000000000000000000" ]; then
    revdiff="$(git merge-base $oldrev $newrev)..$newrev"
fi

for merged in $(git rev-parse --not --all | grep -v $oldrev | git rev-list --reverse --stdin $revdiff) ; do
    echo $merged;
done
